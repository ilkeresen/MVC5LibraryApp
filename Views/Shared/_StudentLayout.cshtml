
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="x-ua-compatible" content="ie=edge">

    <title>Mvc5 Kütüphane - Öğrenci</title>

    <!-- Favicons -->
    <link rel="apple-touch-icon" type="image/png" href="~/VitrinTema/assets/img/icons/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/VitrinTema/assets/img/icons/favicon-16x16.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/VitrinTema/assets/img/icons/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="192x192" href="~/VitrinTema/assets/img/icons/android-chrome-192x192.png">
    <link rel="icon" type="image/png" sizes="512x512" href="~/VitrinTema/assets/img/icons/android-chrome-512x512.png">

    <!-- Select2 -->
    <link rel="stylesheet" href="~/AdminTema/plugins/select2/css/select2.min.css">
    <link rel="stylesheet" href="~/AdminTema/plugins/select2-bootstrap4-theme/select2-bootstrap4.min.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/AdminTema/plugins/fontawesome-free/css/all.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Tempusdominus Bbootstrap 4 -->
    <link rel="stylesheet" href="~/AdminTema/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">
    <!-- iCheck -->
    <link rel="stylesheet" href="~/AdminTema/plugins/icheck-bootstrap/icheck-bootstrap.min.css">
    <!-- JQVMap -->
    <link rel="stylesheet" href="~/AdminTema/plugins/jqvmap/jqvmap.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/AdminTema/dist/css/adminlte.min.css">
    <!-- overlayScrollbars -->
    <link rel="stylesheet" href="~/AdminTema/plugins/overlayScrollbars/css/OverlayScrollbars.min.css">
    <!-- Daterange picker -->
    <link rel="stylesheet" href="~/AdminTema/plugins/daterangepicker/daterangepicker.css">
    <!-- summernote -->
    <link rel="stylesheet" href="~/AdminTema/plugins/summernote/summernote-bs4.css">
    <!-- DataTables -->
    <link rel="stylesheet" href="~/AdminTema/plugins/datatables-bs4/css/dataTables.bootstrap4.min.css">
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
    <link href="~/AdminTema/dist/css/paginationcss.css" rel="stylesheet" />
    <style>
        .btn-circle.btn-sm {
            width: 30px;
            height: 30px;
            padding: 6px 0px;
            border-radius: 15px;
            font-size: 8px;
            text-align: center;
        }

        .btn-circle.btn-md {
            width: 50px;
            height: 50px;
            padding: 7px 10px;
            border-radius: 25px;
            font-size: 10px;
            text-align: center;
        }
    </style>
</head>
<body class="hold-transition sidebar-mini layout-fixed sidebar-collapse">
    <div class="wrapper">
        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">

            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>
            </ul>

            <!-- SEARCH FORM -->
            <form class="form-inline ml-3">
                <div class="input-group input-group-sm">
                    <input class="form-control form-control-navbar" type="search" placeholder="Ara" aria-label="Ara">
                    <div class="input-group-append">
                        <button class="btn btn-navbar" type="submit">
                            <i class="fas fa-search"></i>
                        </button>
                    </div>
                </div>
            </form>

        </nav>
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a href="/Studentpanel/Index/" class="brand-link">
                <img src="~/AdminTema/dist/img/icons/android-chrome-192x192.png" alt="AdminLTE Logo" class="brand-image img-circle elevation-3"
                     style="opacity: .8">
                <span class="brand-text font-weight-light">Mvc5 Kütüphane</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img src="@TempData["UserPhoto"]" class="img-circle elevation-2" alt="User Image">
                    </div>
                    <div class="info">
                        <a href="/Studentpanel/Index/" class="d-block">@TempData["UserNameSurname"]</a>
                    </div>
                </div>
                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <!-- Add icons to the links using the .nav-icon class
                        with font-awesome or any other icon font library -->
                        <li class="nav-item">
                            <a href="/Studentpanel/Index/" class="nav-link">
                                <i class="nav-icon fas fa-user"></i>
                                <p>
                                    Profilim
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Message/Index/" class="nav-link">
                                <i class="nav-icon fas fa-comment"></i>
                                <p>
                                    Mesajlarım
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Studentpanel/MyBooks/" class="nav-link">
                                <i class="nav-icon fas fa-book"></i>
                                <p>
                                    Kitaplarım
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="pages/widgets.html" class="nav-link">
                                <i class="nav-icon fas fa-question"></i>
                                <p>
                                    Yardım
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Studentpanel/LogOut/" class="nav-link">
                                <i class="nav-icon fas fa-sign-out-alt"></i>
                                <p>
                                    Çıkış Yap
                                </p>
                            </a>
                        </li>
                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">

            @RenderBody()

        </div>
        <!-- /.content-wrapper -->
        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Control sidebar content goes here -->
        </aside>
        <!-- /.control-sidebar -->
        <!-- Main Footer -->
        <footer class="main-footer">
            <strong>Copyright &copy; 2014-2022 <a href="https://www.linkedin.com/in/ilker-esen/" target="_blank">İlker ESEN</a>.</strong>
            All rights reserved.
            <div class="float-right d-none d-sm-inline-block">
                <b>Version</b> 3.0.4
            </div>
        </footer>
    </div>
    <!-- ./wrapper -->
    <!-- REQUIRED SCRIPTS -->
    <!-- jQuery -->
    <script src="~/AdminTema/plugins/jquery/jquery.min.js"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="~/AdminTema/plugins/jquery-ui/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button)
    </script>
    <!-- Bootstrap 4 -->
    <script src="~/AdminTema/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- ChartJS -->
    <script src="~/AdminTema/plugins/chart.js/Chart.min.js"></script>
    <!-- Sparkline -->
    <script src="~/AdminTema/plugins/sparklines/sparkline.js"></script>
    <!-- JQVMap -->
    <script src="~/AdminTema/plugins/jqvmap/jquery.vmap.min.js"></script>
    <script src="~/AdminTema/plugins/jqvmap/maps/jquery.vmap.usa.js"></script>
    <!-- jQuery Knob Chart -->
    <script src="~/AdminTema/plugins/jquery-knob/jquery.knob.min.js"></script>
    <!-- daterangepicker -->
    <script src="~/AdminTema/plugins/moment/moment.min.js"></script>
    <script src="~/AdminTema/plugins/moment/locale/tr.js"></script>
    <script src="~/AdminTema/plugins/daterangepicker/daterangepicker.js"></script>
    <!-- Tempusdominus Bootstrap 4 -->
    <script src="~/AdminTema/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>
    <!-- Summernote -->
    <script src="~/AdminTema/plugins/summernote/summernote-bs4.min.js"></script>
    <!-- overlayScrollbars -->
    <script src="~/AdminTema/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/AdminTema/dist/js/adminlte.js"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <script src="~/AdminTema/dist/js/pages/dashboard.js"></script>
    <!-- DataTables -->
    <script src="~/AdminTema/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="~/AdminTema/plugins/datatables-bs4/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/AdminTema/plugins/datatables-responsive/js/dataTables.responsive.min.js"></script>
    <script src="~/AdminTema/plugins/datatables-responsive/js/responsive.bootstrap4.min.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="~/AdminTema/dist/js/demo.js"></script>
    <script src="~/AdminTema/DataTableTr/datatabletr.js"></script>
    <script src="~/AdminTema/crud/crudprocess.js"></script>
    <!-- Select2 -->
    <script src="~/AdminTema/plugins/select2/js/select2.full.min.js"></script>
    <!-- InputMask -->
    <script src="~/AdminTema/plugins/moment/moment.min.js"></script>
    <script src="~/AdminTema/plugins/inputmask/min/jquery.inputmask.bundle.min.js"></script>
    <script>

        $(function () {
            //Initialize Select2 Elements
            $('.select2').select2()
            document.getElementById("BookCategory")[0].setAttribute("disabled", "disabled");
            document.getElementById("BookAuthor")[0].setAttribute("disabled", "disabled");

        })
        document.getElementById("LoanUser")[0].setAttribute("disabled", "disabled");
        document.getElementById("LoanBook")[0].setAttribute("disabled", "disabled");
        document.getElementById("LoanEmployee")[0].setAttribute("disabled", "disabled");

    </script>
    <script>
        //Datemask dd/mm/yyyy
        $('#datemask').inputmask('dd/mm/yyyy', { 'placeholder': 'dd/mm/yyyy' })
        //Datemask3 mm/dd/yyyy
        $('#tell').inputmask('0(999) 999-9999', { 'placeholder': '0(___) ___-____' })
        //BASIM YILI yyyy
        $('#basimyili').inputmask({ "mask": "9999" })
        //Money Euro
        $('[data-mask]').inputmask()
    </script>
    <script>
        $(function () {
            //Add text editor
            $('#compose-textarea').summernote()
        })
    </script>
</body>
</html>
